
--https://martincarstenbach.wordpress.com/2015/07/08/json-support-in-exadata-12-1-2-1-0-and-later/
--https://github.com/tanelpoder/tpt-oracle/blob/master/sqlidx.sql
select sql_id, child_number,
  2  case when io_cell_offload_eligible_bytes = 0 then 'NO' else 'YES' end offloaded,
  3  io_cell_offload_eligible_bytes/power(1024,2) offload_eligible_mb,
  4  io_interconnect_bytes/power(1024,2) interconnect_mb,
  5  io_cell_offload_returned_bytes/power(1024,2) returned_mb,
  6  io_cell_offload_returned_bytes/io_cell_offload_eligible_bytes*100 offload_pct
  7   from v$sql where sql_id = '6j73xcww7hmcw';  
  

select s.name, m.value/1024/1024 mb from v$mystat m, v$sysstat s where m.statistic#=s.statistic# and (s.name like '%physical IO%' or s.name like '%optimized%' or s.name like 'physical%total bytes') order by 2 desc; 
select round(IO_CELL_OFFLOAD_ELIGIBLE_BYTES/1024/1024/1024) eligible, round(IO_CELL_OFFLOAD_RETURNED_BYTES/1024/1024/1024) returned, round(IO_CELL_OFFLOAD_RETURNED_BYTES/(IO_CELL_OFFLOAD_ELIGIBLE_BYTES+1),3)*100 "offload_returned_ratio(%)", a.* from gv$sql a where IO_CELL_OFFLOAD_ELIGIBLE_BYTES > IO_CELL_OFFLOAD_RETURNED_BYTES order by 1 desc;
